<p id="notice"><%= notice %></p>

<h1>Listing Postings</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th>Category</th>
      <th>Rate</th>
      <th>Date range</th>
      <th>Street</th>
      <th>State</th>
      <th>Zip</th>
      <th>Phone</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @postings.each do |posting| %>
      <tr>
        <td><%= posting.title %></td>
        <td><%= posting.description %></td>
        <td><%= posting.category %></td>
        <td><%= posting.rate %></td>
        <td><%= posting.available_dates %></td>
        <td><%= posting.street %></td>
        <td><%= posting.state %></td>
        <td><%= posting.zip %></td>
        <td><%= posting.phone %></td>
        <td><%= posting.email %></td>
        <td><%= link_to 'Show', posting %></td>
        <td><%= link_to 'Edit', edit_posting_path(posting) %></td>
        <td><%= link_to 'Destroy', posting, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Posting', new_posting_path %>
